<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.framework.dao.TInsuranceCommitDao">

	<select id="queryObject" resultType="com.framework.entity.TInsuranceCommitEntity">
		select * from t_insurance_commit where id = #{value}
	</select>

	<select id="queryList" resultType="com.framework.entity.TInsuranceCommitEntity">
		select * from t_insurance_commit where 1=1
		<if test="date != null and date != ''">
			and create_time like "%${date}%"
		</if>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="queryTotal" resultType="int">
		select count(*) from t_finance_commit 
		where 1=1
		<if test="date != null and date != ''">
			and create_time like "%${date}%"
		</if>
	</select>
	 
	<insert id="save" parameterType="com.framework.entity.TInsuranceCommitEntity" useGeneratedKeys="true" keyProperty="id">
		insert into t_insurance_commit
		(
			`finance_id`, 
			`brand_id`,
			`brand_series_id`, 
			`province_id`, 
			`city_id`, 
			`name`, 
			`age`, 
			`idcard_no`, 
			`sex`, 
			`mobile`, 
			`mark`, 
			`create_time`, 
			`update_by`, 
			`update_time`, 
			`status`
		)
		values
		(
			#{financeId}, 
			#{brandId},
			#{brandSeriesId}, 
			#{provinceId}, 
			#{cityId}, 
			#{name}, 
			#{age}, 
			#{idcardNo}, 
			#{sex}, 
			#{mobile}, 
			#{mark}, 
			#{createTime}, 
			#{updateBy}, 
			#{updateTime}, 
			#{status}
		)
	</insert>
	 
	<update id="update" parameterType="com.framework.entity.TInsuranceCommitEntity">
		update t_insurance_commit
		<set>
			<if test="mark != null">`mark` = #{mark}, </if>
			<if test="updateBy != null">`update_by` = #{updateBy}, </if>
			<if test="updateTime != null">`update_time` = #{updateTime}, </if>
			<if test="status != null">`status` = #{status}</if>
		</set>
		where id = #{id}
	</update>
	
	<delete id="delete">
		delete from t_insurance_commit where id = #{value}
	</delete>
	
	<delete id="deleteBatch">
		delete from t_insurance_commit where id in
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>