<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.framework.dao.UserDeviceTokenDao">
    <select id="queryToken" resultType="java.lang.Integer">
        select count(1) from user_device_token where device_token = #{token} and create_time &gt;= #{fromTime} and create_time &lt;= #{toTime}
    </select>
    <insert id="save" parameterType="com.framework.entity.UserDeviceToken" useGeneratedKeys="true" keyProperty="id">
        insert into user_device_token
        (
            `mobile`,
            `device_token`,
            `create_time`,
            `update_time`,
            `code`
        )
        values
        (
            #{mobile},
            #{deviceToken},
            #{createTime},
            #{updateTime},
            #{code}
        )
    </insert>
</mapper>