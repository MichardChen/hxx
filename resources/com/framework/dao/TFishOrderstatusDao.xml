<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.framework.dao.TFishOrderstatusDao">

	<select id="queryObject" resultType="com.framework.entity.TFishOrderstatusEntity">
		select * from t_fish_orderstatus where id = #{value}
	</select>
	<select id="queryByOrderNo"  resultType="com.framework.entity.TFishOrderstatusEntity">
		select * from t_fish_orderstatus where order_no = #{orderNo} and status = #{status} order by create_time desc limit 1
	</select>
	<select id="queryList" resultType="com.framework.entity.TFishOrderstatusEntity">
		select * from t_fish_orderstatus 
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="queryTotal" resultType="int">
		select count(*) from t_fish_orderstatus 
	</select>
	 
	<insert id="save" parameterType="com.framework.entity.TFishOrderstatusEntity">
		insert into t_fish_orderstatus
		(
			`id`, 
			`order_no`, 
			`order_type_cd`, 
			`status`, 
			`create_time`, 
			`update_time`, 
			`mark`,
			`update_by`,
			`params`
		)
		values
		(
			#{id}, 
			#{orderNo}, 
			#{orderTypeCd}, 
			#{status}, 
			#{createTime}, 
			#{updateTime}, 
			#{mark},
			#{updateBy},
			#{params}
		)
	</insert>
	 
	<update id="update" parameterType="com.framework.entity.TFishOrderstatusEntity">
		update t_fish_orderstatus 
		<set>
			<if test="orderNo != null">`order_no` = #{orderNo}, </if>
			<if test="orderTypeCd != null">`order_type_cd` = #{orderTypeCd}, </if>
			<if test="status != null">`status` = #{status}, </if>
			<if test="createTime != null">`create_time` = #{createTime}, </if>
			<if test="updateTime != null">`update_time` = #{updateTime}, </if>
			<if test="mark != null">`mark` = #{mark}, </if>
			<if test="updateBy != null">`update_by` = #{updateBy}, </if>
			<if test="params != null">`params` = #{params}</if>
		</set>
		where id = #{id}
	</update>
	
	<delete id="delete">
		delete from t_fish_orderstatus where id = #{value}
	</delete>
	
	<delete id="deleteBatch">
		delete from t_fish_orderstatus where id in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>