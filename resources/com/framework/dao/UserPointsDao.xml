<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.framework.dao.UserPointsDao">
    <insert id="save" parameterType="com.framework.entity.UserPoints" useGeneratedKeys="true" keyProperty="id">
        insert into user_points
        (
            `user_type_cd`,
            `user_id`,
            `points`,
            `create_time`,
            `update_time`
        )
        values
        (
            #{userTypeCd},
            #{userId},
            #{points},
            #{createTime},
            #{updateTime}
        )
    </insert>
</mapper>